import java.util.*;
public class Main {
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt(); 
        if (n <= 0)
        {
            System.out.println("Invalid Input");
            return;
        }
        double sum = 0;
        for (int i = 0; i < n; i++)
        {
            sum += sc.nextDouble();
        }
        double mean = sum / n;
        System.out.printf("Array Mean Value is %.2f", mean);
    }
}
