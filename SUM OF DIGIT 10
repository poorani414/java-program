import java.util.Scanner;
public class Main
{
    public static void main(String[] args) 
    {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();  
        if (num < 100)
        {
            System.out.println("Invalid Input");
        } 
        else 
        {
            int temp = num;
            int sum = 0;
            while (temp > 0)
            {
                sum += temp % 10;
                temp /= 10;
            }

            System.out.println("Sum of digit is " + sum);
        }
    }
}
